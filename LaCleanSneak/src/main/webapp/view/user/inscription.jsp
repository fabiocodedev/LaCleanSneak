<form class="m-auto my-1 rounded" method="post" action="inscription">
  <div class="m-auto col-md-6">
    <label for="exampleInputNom" class="form-label">Nom :</label>
    <input type="text" class="form-control" id="exampleInputNom" name="lastName">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputPrenom" class="form-label">Prenom :</label>
    <input type="text" class="form-control" id="exampleInputPrenom" name="firstName">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputAdresse" class="form-label">Adresse :</label>
    <input type="text" class="form-control" id="exampleInputAdresse" name="address">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputTelephone" class="form-label">Telephone :</label>
    <input type="text" class="form-control" id="exampleInputTelephone" name="phone">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputEmail" class="form-label">Email :</label>
    <input type="Email" class="form-control" id="exampleInputEmail" name="mail">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputMdp" class="form-label">Mot de passe :</label>
    <input type="password" class="form-control" id="exampleInputMdp" name="password">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputPic" class="form-label">Photo :</label>
    <input type="text" class="form-control" id="exampleInputPic" name="userPicPath">
  </div>
  
  <div class="row justify-content-center m-auto col-md-6"> 
  <button type="submit" class="btn btn-primary mt-3">Submit</button>
  </div>
  
</form>