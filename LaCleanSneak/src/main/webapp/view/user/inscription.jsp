<form class="m-auto my-3 rounded" method="post" action="inscription">
  <div class="m-auto col-md-6">
    <label for="exampleInputNom" class="form-label">Nom :</label>
    <input type="text" class="form-control" id="exampleInputNom" name="nom">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputPrenom" class="form-label">Prenom :</label>
    <input type="text" class="form-control" id="exampleInputPrenom" name="prenom">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputAdresse" class="form-label">Adresse :</label>
    <input type="text" class="form-control" id="exampleInputAdresse" name="adresse">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputTelephone" class="form-label">Telephone :</label>
    <input type="text" class="form-control" id="exampleInputTelephone" name="telephone">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputEmail" class="form-label">Email :</label>
    <input type="Email" class="form-control" id="exampleInputEmail" name="email">
  </div>
  
  <div class="m-auto col-md-6">
    <label for="exampleInputMdp" class="form-label">Mot de passe :</label>
    <input type="password" class="form-control" id="exampleInputMdp" name="mdp">
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
  
</form>